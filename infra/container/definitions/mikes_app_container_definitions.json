[
  {
    "name": "${NAME}",
    "image": "644237782704.dkr.ecr.us-east-2.amazonaws.com/mikes_producao:latest",
    "memory": 512,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8085,
        "hostPort": 8085
      }
    ],
    "environment": [
      {
        "name": "DB_HOST",
        "value": "${DB_HOST}"
      },
      {
        "name": "DB_PORT",
        "value": "${DB_PORT}"
      },
      {
        "name": "DB_NAME",
        "value": "${DB_NAME}"
      },
      {
        "name": "DB_USER",
        "value": "${DB_USER}"
      },
      {
        "name": "DB_PASSWORD",
        "value": "${DB_PASSWORD}"
      },
      {
        "name": "SQS_INIT_PRODUCTION_URL",
        "value": "${SQS_INIT_PRODUCTION_URL}"
      },
      {
        "name": "SNS_PRODUCTION_STATUS_CHANGED_ARN",
        "value": "${SNS_PRODUCTION_STATUS_CHANGED_ARN}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${LOG_GROUP_NAME}",
        "awslogs-region": "us-east-2",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }
]